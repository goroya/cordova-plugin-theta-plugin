        },
        "platforms": [
            "android"
        ]
    },
    "devDependencies": {
        "cordova": "^8.0.0",
        "parcel-bundler": "^1.9.7",
        "parcel-plugin-change-file": "^1.2.7",
        "rimraf": "^2.6.2"
